<!DOCTYPE html>
<html lang="en">
<head>
    <title>Student table data</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
</head>
<body>
<div class="table-data">
    <h1>Student table data</h1>
    <table border="3">
        <tr>
            <th>Student id</th>
            <th>Name</th>
            <th>Surname</th>
            <th>Date of birth</th>
        </tr>

        <%
        if(userData.length!=0){
        userData.forEach(function(data){
        %>
        <tr>
            <td><%=data.id_Student%></td>
            <td><%=data.name %></td>
            <td><%=data.surname %></td>
            <td><%=data.date_of_birth %></td>
        </tr>
        <% }) %>
        <% } else{ %>
            <tr>
                <td colspan="7">No Data Found</td>
            </tr>
        <% } %>
    </table>
</div>
</body>
</html>